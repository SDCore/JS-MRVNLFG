{
  "name": "mrvn",
  "version": "2.0.0",
  "description": "Discord LFG bot using Node.JS and knub/discord.js",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "npx cross-env NODE_ENV=production node dist/index.js",
    "watch": "ts-node src/index.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "lint-fix": "eslint --fix \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "codestyle-check": "prettier --check \"src/**/*.ts\"",
    "build": "npm run format && npm run lint-fix && npx rimraf dist && tsc",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migrate-prod": "npm run typeorm -- migration:run",
    "migrate-dev": "npm run build && npm run typeorm -- migration:run",
    "migrate-rollback-prod": "npm run typeorm -- migration:revert",
    "migrate-rollback-dev": "npm run build && npm run typeorm -- migration:revert"
  },
  "dependencies": {
    "@discordjs/rest": "^0.3.0",
    "@types/js-yaml": "^4.0.5",
    "@types/luxon": "^2.3.0",
    "@types/node": "^17.0.21",
    "bufferutil": "^4.0.6",
    "cross-env": "^7.0.3",
    "discord-api-types": "^0.29.0",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "emoji-regex": "^10.0.1",
    "erlpack": "^0.1.4",
    "fp-ts": "^2.11.9",
    "humanize-duration": "^3.27.1",
    "io-ts": "^2.2.16",
    "js-yaml": "^4.1.0",
    "knub": "^30.0.0-beta.46",
    "luxon": "^2.3.1",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "threads": "^1.7.0",
    "typeorm": "^0.2.31",
    "typescript": "^4.6.2",
    "utf-8-validate": "^5.0.9",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "typescript-eslint": "0.0.1-alpha.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DarkView/JS-MRVNLFG.git"
  },
  "author": "DarkView",
  "license": "GNU AGPLv3"
}
